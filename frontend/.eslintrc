{
  "extends": "airbnb",
  "env": {
    "jest": true,
    "browser": true,
  },
  "rules": {
    "react/jsx-filename-extension": [ 1, { "extensions": [".js", ".jsx"] } ],
    "import/no-extraneous-dependencies": [ "error", {
      "devDependencies": true,
      "optionalDependencies": false,
      "peerDependencies": true,
    }],
    "jsx-a11y/anchor-is-valid": [ "error", {
      "components": [ "Link" ],
      "specialLink": [ "to" ],
    }],
  }
}
